    "@types/mocha": "^10.0.6",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "casper-node-launcher-js": "^0.0.3",
    "chai": "^5.0.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-mocha": "^10.0.0",
    "typescript": "^5.3.3"
  }
}
