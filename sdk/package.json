{
  "name": "@invariant-labs/cspr-sdk",
  "version": "0.1.1",
  "collaborators": [
    "Invariant Labs"
  ],
  "description": "",
  "keywords": [
    "Casper Network",
    "DEX",
    "SDK",
    "odra"
  ],
  "main": "index.js",
  "scripts": {
    "start": "npx tsc && node --experimental-wasm-modules target/src/index.js",
    "start:ts": "ts-node ./src/index.ts",
    "wasm:build": "cd ../clamm-wasm && wasm-pack build --out-dir ../sdk/invariant-cspr-wasm --out-name index && cd ../sdk && npm run wasm:package",
    "wasm:package": "cd ./invariant-cspr-wasm && npm pkg set type='module'",
    "lint": "eslint . --ext .ts",
    "build": "npx tsc",
    "test": "ts-mocha",
    "wasm:strip": "cd contracts && wasm-strip invariant.wasm && wasm-strip erc20.wasm",
    "node:start": "casper-node-launcher-js node v1.5.3",
    "node:start:d": "casper-node-launcher-js node v1.5.3 --daemon",
    "node:stop": "casper-node-launcher-js stop",
    "docs:copy": "cp ../README.md README.md"
  },
  "dependencies": {
    "@casperlabs/ts-results": "^3.3.5",
    "axios": "^1.6.7",
    "blakejs": "^1.2.1",
    "casper-js-sdk": "^2.15.4",
    "chai": "^5.0.3",
    "invariant-cspr-wasm": "file:./invariant-cspr-wasm",
    "ts-node": "^10.9.2",
    "tsimportlib": "^0.0.5",
    "typescript": "^5.3.3"
  },
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/invariant-labs/protocol-cspr",
    "directory": "sdk"
  },
  "devDependencies": {
    "@ethersproject/bignumber": "^5.7.0",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "casper-node-launcher-js": "^0.0.3",
    "chai": "^5.0.0",
    "eslint": "^8.56.0",
    "ts-mocha": "^10.0.0",
    "typescript": "^5.3.3"
  }
}
